{
  "name": "bg-quickstart",
  "version": "1.0.1",
  "description": "The fastest way to pick a start player.",
  "main": "src/index.js",
  "dependencies": {
    "autoprefixer": "10.0.1",
    "cssnano": "4.1.10",
    "framer-motion": "^2.7.7",
    "postcss-cli": "^8.0.0",
    "preact": "^10.4.7",
    "preact-compat": "3.19.0",
    "react": "^16.3.1",
    "react-app-rewired": "2.1.6",
    "react-dom": "^16.3.1",
    "react-scripts": "^3.4.3",
    "react-spring": "8.0.27",
    "tailwindcss": "^1.8.10",
    "typescript": "4.0.3"
  },
  "scripts": {
    "start": "npm run watch:css && react-app-rewired start",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "build": "react-app-rewired build",
    "prebuild": "REACT_APP_PRODUCTION=true npm run build:css",
    "build:css": "tailwindcss build src/css/tailwind.src.css -o src/css/tailwind.css",
    "watch:css": "tailwindcss build src/css/tailwind.src.css -o src/css/tailwind.css"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "16.9.53",
    "customize-cra": "1.0.0"
  }
}
